# H5前端环境配置示例

# 服务器配置
NODE_ENV=development
H5_PORT=3001
BASE_URL=/api/h5

# 数据库配置
DB_HOST=localhost
DB_PORT=3306
DB_USER=root
DB_PASSWORD=password
DB_NAME=skc_service
DB_POOL_LIMIT=10

# JWT配置
JWT_SECRET=h5_jwt_secret_key_change_in_production
JWT_EXPIRES_IN=30d

# 上传文件配置
UPLOAD_DIR=uploads/h5
MAX_FILE_SIZE=5242880 # 5MB

# 日志配置
LOG_LEVEL=info

# 微信配置
WX_APPID=your_wx_appid
WX_SECRET=your_wx_secret 