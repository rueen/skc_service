# SKC服务 - 生产环境配置示例
# 请复制此文件为.env并填写正确的配置值

# 环境
NODE_ENV=production

# 服务端口
ADMIN_PORT=3002
H5_PORT=3001

# 基础URL
ADMIN_BASE_URL=/api/admin
H5_BASE_URL=/api/h5

# 数据库配置
DB_HOST=localhost
DB_PORT=3306
DB_USER=root
DB_PASSWORD=
DB_NAME=skc
DB_POOL_LIMIT=10

# JWT配置
JWT_SECRET=
JWT_EXPIRES_IN=1d

# 日志配置
LOG_LEVEL=info

# 支付配置
PAYMENT_API_URL=https://72pay.la2568.site/api/daifu
PAYMENT_QUERY_API_URL=https://72pay.la2568.site/api/query

# API签名配置
API_SIGN_SECRET=
API_SIGN_EXPIRE_TIME=300

# OSS配置
OSS_ACCESS_KEY_ID=
OSS_ACCESS_KEY_SECRET=
OSS_REGION=
OSS_BUCKET=
H5_OSS_DIR=skc/h5
ADMIN_OSS_DIR=skc/admin

# 部署配置
DEPLOY_HOST=
DEPLOY_REPO=git@github.com:rueen/skc_service.git