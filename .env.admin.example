# 管理后台环境配置示例

# 服务器配置
NODE_ENV=development
ADMIN_PORT=3002
BASE_URL=/api/support

# 数据库配置
DB_HOST=localhost
DB_PORT=3306
DB_USER=root
DB_PASSWORD=password
DB_NAME=skc_service
DB_POOL_LIMIT=10

# JWT配置
JWT_SECRET=admin_jwt_secret_key_change_in_production
JWT_EXPIRES_IN=1d

# 上传文件配置
UPLOAD_DIR=uploads
MAX_FILE_SIZE=5242880 # 5MB

# 日志配置
LOG_LEVEL=info 